import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const EEnumWithStringsSchema = {
    enum: ['Success', 'Warning', 'Error'],
    type: 'string',
};

export type EEnumWithStrings = any; // JSON Schema doesn't provide type inference

export function validateEEnumWithStrings(
    data: unknown,
): { valid: true; data: EEnumWithStrings } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(EEnumWithStringsSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as EEnumWithStrings };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateEEnumWithStringsSync(data: unknown): EEnumWithStrings {
    const validate = ajv.compile(EEnumWithStringsSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as EEnumWithStrings;
}
