import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const EEnumWithNumbersSchema = {
    enum: [1, 2, 3, 1.1, 1.2, 1.3, 100, 200, 300, -100, -200, -300, -1.1, -1.2, -1.3],
    type: 'number',
};

export type EEnumWithNumbers = any; // JSON Schema doesn't provide type inference

export function validateEEnumWithNumbers(
    data: unknown,
): { valid: true; data: EEnumWithNumbers } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(EEnumWithNumbersSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as EEnumWithNumbers };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateEEnumWithNumbersSync(data: unknown): EEnumWithNumbers {
    const validate = ajv.compile(EEnumWithNumbersSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as EEnumWithNumbers;
}
