import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const EEnumWithStringsWithTypeSchema = {
    enum: ['VALUE1', 'VALUE2'],
    type: 'string',
};

export type EEnumWithStringsWithType = any; // JSON Schema doesn't provide type inference

export function validateEEnumWithStringsWithType(
    data: unknown,
): { valid: true; data: EEnumWithStringsWithType } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(EEnumWithStringsWithTypeSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as EEnumWithStringsWithType };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateEEnumWithStringsWithTypeSync(data: unknown): EEnumWithStringsWithType {
    const validate = ajv.compile(EEnumWithStringsWithTypeSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as EEnumWithStringsWithType;
}
