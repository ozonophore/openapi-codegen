    const path = options.path.replace('{api-version}', config.VERSION).replace(/[:]/g, '_');
    const url = \`\${config.BASE}\${path}\`;

    if (options.query) {
        return \`\${url}\${getQueryString(options.query)}\`;
    }
    return url;
}

