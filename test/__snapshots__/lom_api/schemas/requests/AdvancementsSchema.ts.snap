import Ajv, { ErrorObject } from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const AdvancementsSchema = {
    type: 'object',
    properties: {},
    additionalProperties: false,
};

export type Advancements = any; // JSON Schema doesn't provide type inference

export function validateAdvancements(
    data: unknown,
): { valid: true; data: Advancements } | { valid: false; errors: ErrorObject[] } {
    const validate = ajv.compile(AdvancementsSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as Advancements };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateAdvancementsSync(data: unknown): Advancements {
    const validate = ajv.compile(AdvancementsSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as Advancements;
}
