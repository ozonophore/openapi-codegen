import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const PathwaysSchema = {
    type: 'object',
    properties: {},
    additionalProperties: false,
};

export type Pathways = any; // JSON Schema doesn't provide type inference

export function validatePathways(
    data: unknown,
): { valid: true; data: Pathways } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(PathwaysSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as Pathways };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validatePathwaysSync(data: unknown): Pathways {
    const validate = ajv.compile(PathwaysSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as Pathways;
}
