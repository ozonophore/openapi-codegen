import Ajv, { ErrorObject } from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const SequenceByIdSchema = {
    type: 'object',
    properties: {},
    additionalProperties: false,
};

export type SequenceById = any; // JSON Schema doesn't provide type inference

export function validateSequenceById(
    data: unknown,
): { valid: true; data: SequenceById } | { valid: false; errors: ErrorObject[] } {
    const validate = ajv.compile(SequenceByIdSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as SequenceById };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateSequenceByIdSync(data: unknown): SequenceById {
    const validate = ajv.compile(SequenceByIdSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as SequenceById;
}
