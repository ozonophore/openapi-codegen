import Ajv, { ErrorObject } from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const PathwaysSequencesSchema = {
    type: 'object',
    properties: {},
    additionalProperties: false,
};

export type PathwaysSequences = any; // JSON Schema doesn't provide type inference

export function validatePathwaysSequences(
    data: unknown,
): { valid: true; data: PathwaysSequences } | { valid: false; errors: ErrorObject[] } {
    const validate = ajv.compile(PathwaysSequencesSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as PathwaysSequences };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validatePathwaysSequencesSync(data: unknown): PathwaysSequences {
    const validate = ajv.compile(PathwaysSequencesSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as PathwaysSequences;
}
