import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const PathwaysByIdSchema = {
    type: 'object',
    properties: {},
    additionalProperties: false,
};

export type PathwaysById = any; // JSON Schema doesn't provide type inference

export function validatePathwaysById(
    data: unknown,
): { valid: true; data: PathwaysById } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(PathwaysByIdSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as PathwaysById };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validatePathwaysByIdSync(data: unknown): PathwaysById {
    const validate = ajv.compile(PathwaysByIdSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as PathwaysById;
}
