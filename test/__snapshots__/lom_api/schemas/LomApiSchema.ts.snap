import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const LomApiSchema = {
    type: 'object',
    properties: {},
    additionalProperties: false,
};

export type LomApi = any; // JSON Schema doesn't provide type inference

export function validateLomApi(
    data: unknown,
): { valid: true; data: LomApi } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(LomApiSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as LomApi };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateLomApiSync(data: unknown): LomApi {
    const validate = ajv.compile(LomApiSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as LomApi;
}
