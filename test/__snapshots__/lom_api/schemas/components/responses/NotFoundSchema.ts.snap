import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const NotFoundSchema = {
    type: 'object',
    properties: {},
    additionalProperties: false,
};

export type NotFound = any; // JSON Schema doesn't provide type inference

export function validateNotFound(
    data: unknown,
): { valid: true; data: NotFound } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(NotFoundSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as NotFound };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateNotFoundSync(data: unknown): NotFound {
    const validate = ajv.compile(NotFoundSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as NotFound;
}
