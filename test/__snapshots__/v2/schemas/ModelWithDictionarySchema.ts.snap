import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const IModelWithDictionarySchema = {
    type: 'object',
    properties: {
        prop: {
            type: 'object',
            additionalProperties: true,
        },
    },

    additionalProperties: false,
};

export type IModelWithDictionary = any; // JSON Schema doesn't provide type inference

export function validateIModelWithDictionary(
    data: unknown,
): { valid: true; data: IModelWithDictionary } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(IModelWithDictionarySchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as IModelWithDictionary };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateIModelWithDictionarySync(data: unknown): IModelWithDictionary {
    const validate = ajv.compile(IModelWithDictionarySchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as IModelWithDictionary;
}
