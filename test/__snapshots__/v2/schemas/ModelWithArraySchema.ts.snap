import { IModelWithStringSchema } from './ModelWithString';

import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const IModelWithArraySchema = {
    type: 'object',
    properties: {
        prop: {
            type: 'array',
            items: {
                type: 'object',
            },
        },
        propWithFile: {
            type: 'array',
            items: true,
        },
        propWithNumber: {
            type: 'array',
            items: true,
        },
    },

    additionalProperties: false,
};

export type IModelWithArray = any; // JSON Schema doesn't provide type inference

export function validateIModelWithArray(
    data: unknown,
): { valid: true; data: IModelWithArray } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(IModelWithArraySchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as IModelWithArray };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateIModelWithArraySync(data: unknown): IModelWithArray {
    const validate = ajv.compile(IModelWithArraySchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as IModelWithArray;
}
