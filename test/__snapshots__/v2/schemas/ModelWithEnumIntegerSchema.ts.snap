import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const IModelWithEnumIntegerSchema = {
    type: 'object',
    properties: {
        prop: {
            enum: [1, 2, 3],
            type: 'number',
        },
    },

    additionalProperties: false,
};

export type IModelWithEnumInteger = any; // JSON Schema doesn't provide type inference

export function validateIModelWithEnumInteger(
    data: unknown,
): { valid: true; data: IModelWithEnumInteger } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(IModelWithEnumIntegerSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as IModelWithEnumInteger };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateIModelWithEnumIntegerSync(data: unknown): IModelWithEnumInteger {
    const validate = ajv.compile(IModelWithEnumIntegerSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as IModelWithEnumInteger;
}
