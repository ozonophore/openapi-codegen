import { IModelWithStringSchema } from './ModelWithString';

import Ajv from 'ajv';
import addFormats from 'ajv-formats';

const ajv = new Ajv({ allErrors: true });
addFormats(ajv);

export const TModelThatExtendsSchema = {
    allOf: [
        IModelWithStringSchema,
        {
            type: 'object',
            properties: {
                propExtendsA: {
                    type: 'string',
                },
                propExtendsB: IModelWithStringSchema,
            },

            additionalProperties: false,
        },
    ],
};

export type TModelThatExtends = any; // JSON Schema doesn't provide type inference

export function validateTModelThatExtends(
    data: unknown,
): { valid: true; data: TModelThatExtends } | { valid: false; errors: Ajv.ErrorObject[] } {
    const validate = ajv.compile(TModelThatExtendsSchema);
    const valid = validate(data);
    if (valid) {
        return { valid: true, data: data as TModelThatExtends };
    }
    return { valid: false, errors: validate.errors || [] };
}

export function validateTModelThatExtendsSync(data: unknown): TModelThatExtends {
    const validate = ajv.compile(TModelThatExtendsSchema);
    const valid = validate(data);
    if (!valid) {
        throw new Error(ajv.errorsText(validate.errors));
    }
    return data as TModelThatExtends;
}
