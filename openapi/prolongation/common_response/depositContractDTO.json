{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Deposit Contact",
  "type": "object",
  "description": "Данные по продукту",
  "properties": {
    "contractNumber": {
      "type": "string",
      "description": "Номер продукта в ППРБ"
    },
    "contractDate": {
      "type": "string",
      "format": "date",
      "description": "Дата начала действия продукта"
    },
    "contractEndDate": {
      "type": "string",
      "format": "date",
      "description": "Дата окончания действия продукта"
    }
  },
  "required": [
    "contractNumber",
    "contractDate",
    "contractEndDate"
  ],
  "additionalProperties": false
}