{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Проверка введенного СМС кода для подписания",
  "type": "object",
  "required": [
    "documentGUID",
    "cryptoProfileId",
    "code"
  ],
  "properties": {
    "documentGUID": {
      "title": "GUID документа",
      "type": "string",
      "maxLength": 255
    },
    "cryptoProfileId": {
      "title": "Идентфикатор криптопрофиля",
      "type": "integer",
      "format": "int64"
    },
    "code": {
      "title": "СМС код",
      "type": "string",
      "maxLength": 100
    }
  },
  "additionalProperties": false
}
