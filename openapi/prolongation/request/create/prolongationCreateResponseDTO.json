{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Response for prolongation creation",
  "description": "Ответ на создание пролонгации",
  "type": "object",
  "properties": {
    "account": {
      "$ref": "../../common_response/accountDTO.json"
    },
    "accountReturn": {
      "$ref": "../../common_response/accountDTO.json"
    },
    "specialCondition": {
      "$ref": "../../common_response/specialConditionDTO.json",
      "description": "Признак спец. условий (программа лояльности)"
    },
    "depositAmount": {
      "$ref": "../../common_response/depositAmountDTO.json"
    },
    "product": {
      "$ref": "../../common_response/productDTO.json"
    },
    "docData": {
      "$ref": "../../common_response/docDataDTO.json"
    },
    "depositContract": {
      "$ref": "../../common_response/depositContractDTO.json"
    },
    "depositTermDate": {
      "type": "string",
      "format": "date",
      "description": "Дата списания средств"
    },
    "depositEndDate": {
      "type": "string",
      "format": "date",
      "description": "Дата окончания депозита"
    },
    "isEqualAccounts": {
      "type": "boolean",
      "description": "Признак совпадения счетов возврата и списания"
    },
    "replenishmentType": {
      "type": "string",
      "description": "Тип зачисления"
    },
    "paymentPeriodCode": {
      "type": "string",
      "description": "Периодичность выплаты"
    },
    "rate": {
      "type": "number",
      "description": "Ставка"
    },
    "depositGUID": {
      "type": "string",
      "description": "ID родительского депозита"
    },
    "term": {
      "type": "integer",
      "description": "Срок депозита"
    },
    "isReplenished": {
      "type": "boolean",
      "description": "Признак пополняемости"
    },
    "isRecallable": {
      "type": "boolean",
      "description": "Признак отзываемости"
    },
    "organizationData": {
      "$ref": "../../common_response/organizationDataDTO.json"
    },
    "errors": {
      "$ref": "../../common_response/errorsDTO.json"
    }
  },
  "required": [
    "accountReturn",
    "specialCondition",
    "depositAmount",
    "docData",
    "product",
    "organizationData",
    "depositContract",
    "depositTermDate",
    "depositEndDate",
    "isEqualAccounts",
    "paymentPeriodCode",
    "replenishmentType",
    "rate",
    "term",
    "isReplenished",
    "isRecallable"
  ],
  "additionalProperties": false
}