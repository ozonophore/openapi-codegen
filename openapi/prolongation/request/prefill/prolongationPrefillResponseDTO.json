{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "title": "Response for prolongation prefill",
  "description": "Ответ на префилл пролонгации",
  "type": "object",
  "properties": {
    "accountReplenishment": {
      "$ref": "../../common_response/accountDTO.json",
      "description": "Счет списания в депозит из продукта"
    },
    "accountReturn": {
      "$ref": "../../common_response/accountDTO.json",
      "description": "Счет возврата из продукта"
    },
    "specialCondition": {
      "$ref": "../../common_response/specialConditionDTO.json"
    },
    "depositAmount": {
      "$ref": "../../common_response/depositAmountDTO.json"
    },
    "product": {
      "$ref": "../../common_response/productDTO.json"
    },
    "depositTermDate": {
      "type": "string",
      "format": "date",
      "description": "Дата списания средств"
    },
    "docData": {
      "$ref": "../../common_request/docDataDTO.json",
      "description": "Данные по заявлению"
    },
    "depositContract": {
      "$ref": "../../common_response/depositContractDTO.json"
    },
    "term": {
      "type": "integer",
      "description": "Срок депозита"
    },
    "replenishmentType": {
      "type": "string",
      "description": "Тип зачисления"
    },
    "errors": {
      "$ref": "../../common_response/errorsDTO.json"
    }
  },
  "required": [
    "accountReturn",
    "specialCondition",
    "depositAmount",
    "product",
    "depositTermDate",
    "docData",
    "depositContract",
    "term",
    "replenishmentType"
