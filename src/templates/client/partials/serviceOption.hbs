method: '{{{method}}}',
url: `{{{path}}}`,
{{#if parametersHeader}}
    headers: {
    {{#each parametersHeader}}
        '{{{prop}}}': String({{{name}}}),
    {{/each}}
    {{#if parametersCookie}}
        Cookie: [
        {{#each parametersCookie}}
            `{{{prop}}}=` + String({{{name}}}),
        {{/each}}
        ].join('; '),
    {{/if}}
    {{#if parametersBody}}
        {{#if parametersBody.mediaType}}
            'Content-Type': '{{{parametersBody.mediaType}}}',
        {{/if}}
    {{/if}}
    },
{{else}}
    {{#if parametersCookie}}
        headers: {
        Cookie: [
        {{#each parametersCookie}}
            `{{{prop}}}=` + String({{{name}}}),
        {{/each}}
        ].join('; '),
        {{#if parametersBody}}
            {{#if parametersBody.mediaType}}
                'Content-Type': '{{{parametersBody.mediaType}}}',
            {{/if}}
        {{/if}}
        },
    {{else}}
        {{#if parametersBody}}
            {{#if parametersBody.mediaType}}
                headers: {
                    'Content-Type': '{{{parametersBody.mediaType}}}',
                },
            {{/if}}
        {{/if}}
    {{/if}}
{{/if}}
{{#if parametersQuery}}
    query: {
    {{#each parametersQuery}}
        '{{{prop}}}': {{{name}}},
    {{/each}}
    },
{{/if}}
{{#if parametersForm}}
    body: {
    {{#each parametersForm}}
        '{{{prop}}}': {{{name}}},
    {{/each}}
    },
{{else}}
    {{#if parametersBody}}
        body: {{{parametersBody.name}}},
    {{/if}}
{{/if}}