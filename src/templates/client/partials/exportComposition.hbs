{{#if description}}
/**
 * {{{description}}}
 */
{{/if}}
export type {{{name}}} = {{>type parent=name}};
{{#if enums}}
{{#unless @root.useUnionTypes}}

export namespace {{{name}}} {

    {{#each enums}}
    {{#if description}}
    /**
     * {{{description}}}
     */
    {{/if}}
    {{#if alias}}
    export enum {{{alias}}} {
    {{else}}
    export enum {{{name}}} {
    {{/if}}
        {{#each enum}}
        {{{name}}} = {{{value}}},
        {{/each}}
    }

    {{/each}}

}
{{/unless}}
{{/if}}
