{{#equals export 'one-of'}}
{
    oneOf: [
{{#each properties}}
        {{>jsonschema/jsonschemaSchema}}{{#unless @last}},{{/unless}}
{{/each}}
    ]{{#if isNullable}},
    nullable: true{{/if}}
}
{{else equals export 'any-of'}}
{
    anyOf: [
{{#each properties}}
        {{>jsonschema/jsonschemaSchema}}{{#unless @last}},{{/unless}}
{{/each}}
    ]{{#if isNullable}},
    nullable: true{{/if}}
}
{{else equals export 'all-of'}}
{
    allOf: [
{{#each properties}}
        {{>jsonschema/jsonschemaSchema}}{{#unless @last}},{{/unless}}
{{/each}}
    ]{{#if isNullable}},
    nullable: true{{/if}}
}
{{/equals}}