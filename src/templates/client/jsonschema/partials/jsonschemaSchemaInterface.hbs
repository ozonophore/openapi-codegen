{
    type: 'object',
{{#if properties}}
    properties: {
{{#each properties}}
        {{{name}}}: {{>jsonschemaSchema}}{{#unless @last}},{{/unless}}
{{/each}}
    },
{{getRequiredFields properties}}
{{#if maxProperties}}
    maxProperties: {{{maxProperties}}},
{{/if}}
{{#if minProperties}}
    minProperties: {{{minProperties}}},
{{/if}}
{{#if isNullable}}
    nullable: true,
{{/if}}
    additionalProperties: false
{{else}}
{{#equals emptySchemaStrategy 'semantic'}}
{{#if maxProperties}}
    maxProperties: {{{maxProperties}}},
{{/if}}
{{#if minProperties}}
    minProperties: {{{minProperties}}},
{{/if}}
{{#if isNullable}}
    nullable: true,
{{/if}}
    additionalProperties: true
{{else}}
    properties: {},
{{#if maxProperties}}
    maxProperties: {{{maxProperties}}},
{{/if}}
{{#if minProperties}}
    minProperties: {{{minProperties}}},
{{/if}}
{{#if isNullable}}
    nullable: true,
{{/if}}
    additionalProperties: false
{{/equals}}
{{/if}}
}
